{
  "name": "client",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "type": "module",
  "scripts": {
    "android": "APP_VARIANT=development expo run:android",
    "ios": "APP_VARIANT=development expo run:ios",
    "start": "APP_VARIANT=development expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "check-types": "tsc --noEmit",
    "web": "expo start --web",
    "prepare": "husky"
  },
  "dependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@expo/cli": "^0.22.18",
    "@expo/server": "^0.5.3",
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.14",
    "@shopify/flash-list": "1.7.3",
    "@supabase/supabase-js": "^2.49.1",
    "babel-plugin-inline-import": "^3.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-kit": "^0.30.5",
    "drizzle-orm": "^0.40.0",
    "expo": "^52.0.37",
    "expo-application": "~6.0.2",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-drizzle-studio-plugin": "^0.1.1",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-sqlite": "~15.1.2",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.3",
    "expo-web-browser": "~14.0.2",
    "husky": "^9.1.7",
    "lucide-react-native": "^0.474.0",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-notifier": "^2.0.0",
    "react-native-reanimated": "3.16.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.8.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "tailwind-merge": "^3.0.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@types/react": "~18.3.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^12.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "lucide-react-native",
          "@expo/cli",
          "date-fns",
          "date-fns-tz",
          "drizzle-kit",
          "drizzle-orm",
          "expo-drizzle-studio-plugin"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}
